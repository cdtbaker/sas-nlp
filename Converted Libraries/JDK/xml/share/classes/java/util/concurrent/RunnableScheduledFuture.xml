        <interface name="RunnableScheduledFuture">
            <type_params>
                <type_param name="V"/>
            </type_params>
            <implements interface="RunnableFuture">
                <type_params>
                    <type_param name="V"/>
                </type_params>
            </implements>
            <implements interface="ScheduledFuture">
                <type_params>
                    <type_param name="V"/>
                </type_params>
