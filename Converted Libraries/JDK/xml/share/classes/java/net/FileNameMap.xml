    <source package="java.net">
        <interface name="FileNameMap">
            <javadoc line="2">
                A simple interface which provides a mechanism to map
                  between a file name and a MIME type string.                
                <author>
                    Steven B. Byrne                    
                </author>
                <since>
                    JDK1.1                    
                </since>
            </javadoc>
            <method name="getContentTypeFor" type="String" line="9"/>
            <javadoc line="9">
                Gets the MIME type for the specified file name.                
                <param>
                    fileName the specified file name                    
                </param>
                <return>
                    a <code>String</code> indicating the MIME
                      type for the specified file name.                    
                </return>
            </javadoc>
        </interface>
    </source>