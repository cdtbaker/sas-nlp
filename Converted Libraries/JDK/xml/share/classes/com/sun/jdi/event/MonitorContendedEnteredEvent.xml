    <source package="com.sun.jdi.event">
        <import package="com.sun.jdi"/>
        <interface name="MonitorContendedEnteredEvent">
            <implements interface="LocatableEvent"/>
            <javadoc line="3">
                Notification that a thread in the target VM is entering a monitor
                  after waiting for it to be released by another thread.
                  &lt;P&gt;                
                <see>
                    EventQueue                    
                </see>
                <see>
                    MonitorContendedEnterEvent                    
                </see>
                <author>
                    Swamy Venkataramanappa                    
                </author>
                <since>
                    1.6                    
                </since>
            </javadoc>
            <method name="thread" type="ThreadReference" line="13"/>
            <javadoc line="13">
                Returns the thread in which this event has occurred.
                  &lt;p&gt;                
                <return>
                    a {@link ThreadReference} which mirrors the event&apos;s thread in
                      the target VM.                    
                </return>
            </javadoc>
            <method name="monitor" type="ObjectReference" line="20"/>
            <javadoc line="20">
                Returns the monitor that was entered.                
                <return>
                    an {@link ObjectReference} for the monitor.                    
                </return>
            </javadoc>
        </interface>
