        <import package="com.sun.jdi"/>
        <interface name="MonitorWaitEvent">
            <implements interface="LocatableEvent"/>
            <javadoc line="3">
                Notification that a thread in the target VM is about to
                  wait on a monitor object.
                  &lt;P&gt;                
                <see>
                    EventQueue                    
                </see>
                <see>
                    MonitorWaitedEvent                    
                </see>
                <author>
                    Swamy Venkataramanappa                    
                </author>
                <since>
                    1.6                    
                </since>
            </javadoc>
            <method name="thread" type="ThreadReference" line="13"/>
            <javadoc line="13">
                Returns the thread in which monitor wait event has occurred.
                  &lt;p&gt;                
                <return>
                    a {@link ThreadReference} which mirrors the event&apos;s thread in
                      the target VM.                    
                </return>
            </javadoc>
            <method name="monitor" type="ObjectReference" line="20"/>
            <javadoc line="20">
                Returns the monitor object that the thread about to wait.                
                <return>
                    an {@link ObjectReference} for the monitor.                    
                </return>
            </javadoc>
            <method name="timeout" type="long" line="25"/>
            <javadoc line="25">
                Returns the number of millisecond the thread will wait.                
                <return>
                    a &lt;code&gt;jlong&lt;/code&gt; containing monitor wait time in milliseconds.                    
                </return>
            </javadoc>
        </interface>
