    <source package="com.sun.jdi.event">
        <import package="com.sun.jdi"/>
        <interface name="MonitorWaitedEvent">
            <implements interface="LocatableEvent"/>
            <javadoc line="3">
                Notification that a thread in the target VM has finished
                  waiting on an monitor object.
                  &lt;P&gt;                
                <see>
                    EventQueue                    
                </see>
                <see>
                    MonitorWaitEvent                    
                </see>
                <author>
                    Swamy Venkataramanappa                    
                </author>
                <since>
                    1.6                    
                </since>
            </javadoc>
            <method name="thread" type="ThreadReference" line="13"/>
            <javadoc line="13">
                Returns the thread in which this event has occurred.
                  &lt;p&gt;                
                <return>
                    a {@link ThreadReference} which mirrors the event's thread in
                      the target VM.                    
                </return>
            </javadoc>
            <method name="monitor" type="ObjectReference" line="20"/>
            <javadoc line="20">
                Returns the monitor object this thread waited on.                
                <return>
                    an {@link ObjectReference} for the monitor.                    
                </return>
            </javadoc>
            <method name="timedout" type="boolean" line="25"/>
            <javadoc line="25">
                Returns whether the wait has timed out or been interrupted.                
                <return>
                    <code>true</code> if the wait is timed out.                    
                </return>
            </javadoc>
        </interface>
    </source>