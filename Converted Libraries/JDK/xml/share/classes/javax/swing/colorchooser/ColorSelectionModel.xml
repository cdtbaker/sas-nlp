        <import package="javax.swing"/>
        <import package="javax.swing.event"/>
        <import package="java.awt.Color"/>
        <interface name="ColorSelectionModel">
            <javadoc line="5">
                A model that supports selecting a &lt;code&gt;Color&lt;/code&gt;.                
                <author>
                    Steve Wilson                    
                </author>
                <see>
                    java.awt.Color                    
                </see>
            </javadoc>
            <method name="getSelectedColor" type="Color" line="11"/>
            <javadoc line="11">
                Returns the selected &lt;code&gt;Color&lt;/code&gt; which should be
                  non-&lt;code&gt;null&lt;/code&gt;.                
                <return>
                    the selected <code>Color</code>                    
                </return>
                <see>
                    #setSelectedColor                    
                </see>
            </javadoc>
            <method name="setSelectedColor" type="void" line="18"/>
            <javadoc line="18">
                Sets the selected color to &lt;code&gt;color&lt;/code&gt;.
                  Note that setting the color to &lt;code&gt;null&lt;/code&gt;
                  is undefined and may have unpredictable results.
                  This method fires a state changed event if it sets the
                  current color to a new non-&lt;code&gt;null&lt;/code&gt; color.                
                <param>
                    color the new <code>Color</code>                    
                </param>
                <see>
                    #getSelectedColor                    
                </see>
                <see>
                    #addChangeListener                    
                </see>
            </javadoc>
            <method name="addChangeListener" type="void" line="29"/>
            <javadoc line="29">
                Adds &lt;code&gt;listener&lt;/code&gt; as a listener to changes in the model.                
                <param>
                    listener the <code>ChangeListener</code> to be added                    
                </param>
            </javadoc>
            <method name="removeChangeListener" type="void" line="34"/>
            <javadoc line="34">
                Removes &lt;code&gt;listener&lt;/code&gt; as a listener to changes in the model.                
                <param>
                    listener the <code>ChangeListener</code> to be removed                    
                </param>
            </javadoc>
        </interface>
