        <import package="javax.swing.Action"/>
        <import package="javax.swing.KeyStroke"/>
        <interface name="Keymap">
            <javadoc line="4">
                A collection of bindings of KeyStrokes to actions.  The
                  bindings are basically name-value pairs that potentially
                  resolve in a hierarchy.                
                <author>
                    Timothy Prinzing                    
                </author>
            </javadoc>
            <method name="getName" type="String" line="11"/>
            <javadoc line="11">
                Fetches the name of the set of key-bindings.                
                <return>
                    the name                    
                </return>
            </javadoc>
            <method name="getDefaultAction" type="Action" line="16"/>
            <javadoc line="16">
                Fetches the default action to fire if a
                  key is typed (i.e. a KEY_TYPED KeyEvent is received)
                  and there is no binding for it.  Typically this
                  would be some action that inserts text so that
                  the keymap doesn&apos;t require an action for each
                  possible key.                
                <return>
                    the default action                    
                </return>
            </javadoc>
            <method name="setDefaultAction" type="void" line="26"/>
            <javadoc line="26">
                Set the default action to fire if a key is typed.                
                <param>
                    a the action                    
                </param>
            </javadoc>
            <method name="getAction" type="Action" line="31"/>
            <javadoc line="31">
                Fetches the action appropriate for the given symbolic
                  event sequence.  This is used by JTextController to
                  determine how to interpret key sequences.  If the
                  binding is not resolved locally, an attempt is made
                  to resolve through the parent keymap, if one is set.                
                <param>
                    key the key sequence                    
                </param>
                <return>
                    the action associated with the key
                      sequence if one is defined, otherwise <code>null</code>                    
                </return>
            </javadoc>
            <method name="getBoundKeyStrokes" type="KeyStroke[]" line="42"/>
            <javadoc line="42">
                Fetches all of the keystrokes in this map that
                  are bound to some action.                
                <return>
                    the list of keystrokes                    
                </return>
            </javadoc>
            <method name="getBoundActions" type="Action[]" line="48"/>
            <javadoc line="48">
                Fetches all of the actions defined in this keymap.                
                <return>
                    the list of actions                    
                </return>
            </javadoc>
            <method name="getKeyStrokesForAction" type="KeyStroke[]" line="53"/>
            <javadoc line="53">
                Fetches the keystrokes that will result in
                  the given action.                
                <param>
                    a the action                    
                </param>
                <return>
                    the list of keystrokes                    
                </return>
            </javadoc>
            <method name="isLocallyDefined" type="boolean" line="60"/>
            <javadoc line="60">
                Determines if the given key sequence is locally defined.                
                <param>
                    key the key sequence                    
                </param>
                <return>
                    true if the key sequence is locally defined else false                    
                </return>
            </javadoc>
            <method name="addActionForKeyStroke" type="void" line="66"/>
            <javadoc line="66">
                Adds a binding to the keymap.                
                <param>
                    key the key sequence                    
                </param>
                <param>
                    a the action                    
                </param>
            </javadoc>
