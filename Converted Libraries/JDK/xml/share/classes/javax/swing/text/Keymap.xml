            <javadoc line="72">
                Removes a binding from the keymap.                
                <param>
                    keys the key sequence                    
                </param>
            </javadoc>
            <method name="removeBindings" type="void" line="77"/>
            <javadoc line="77">
                Removes all bindings from the keymap.                
            </javadoc>
