            <method name="solve" type="void" line="46">
                <comment line="47">
                    for( int j = 0; j &lt; numCols; j++ ) {                    
                </comment>
                <comment line="47">
                    for( int i = 0; i &lt; this.numCols; i++ ) vv[i] = dataB[i*numCols+j];                    
                </comment>
                <comment line="47">
                    decomp._solveVectorInternal(vv);                    
                </comment>
                <comment line="47">
                    for( int i = 0; i &lt; this.numCols; i++ ) dataX[i*numCols+j] = vv[i];                    
                </comment>
                <comment line="47">
                    }                    
                </comment>
