    <javadoc>
      <text>* Internal weights Butcher array.</text>
    </javadoc>
    <declaration type="double[][]" name="STATIC_A" />
    <javadoc>
      <text>* Propagation weights Butcher array.</text>
    </javadoc>
    <declaration type="double[]" name="STATIC_B" />
