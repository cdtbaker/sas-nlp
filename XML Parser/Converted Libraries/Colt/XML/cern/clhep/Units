            <comment line="12">
                Electric charge [Q]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                blic static final double eplus = 1. ;		// positron charge                
            </comment>
            <comment line="12">
                blic static final double e_SI  = 1.60217733e-19;	// positron charge in coulomb                
            </comment>
            <comment line="12">
                blic static final double coulomb = eplus/e_SI;	// coulomb = 6.24150 e+18 * eplus                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                Energy [E]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                blic static final double joule = electronvolt/e_SI;	// joule = 6.24150 e+12 * MeV                
            </comment>
            <comment line="12">
                symbols                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                Mass [E][T^2][L^-2]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                symbols                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                Power [E][T^-1]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                blic static final double watt = joule/second;	// watt = 6.24150 e+3 * MeV/ns                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                Force [E][L^-1]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                blic static final double newton = joule/meter;	// newton = 6.24150 e+9 * MeV/mm                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                Pressure [E][L^-3]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                blic static final double hep_pascal = newton/m2;	   // pascal = 6.24150 e+3 * MeV/mm3                
            </comment>
            <comment line="12">
                blic static final double bar        = 100000*pascal; // bar    = 6.24150 e+8 * MeV/mm3                
            </comment>
            <comment line="12">
                blic static final double atmosphere = 101325*pascal; // atm    = 6.32420 e+8 * MeV/mm3                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                Electric current [Q][T^-1]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                blic static final double      ampere = coulomb/second; // ampere = 6.24150 e+9 * eplus/ns                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                Electric potential [E][Q^-1]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                Electric resistance [E][T][Q^-2]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                blic static final double ohm = volt/ampere;	// ohm = 1.60217e-16*(MeV/eplus)/(eplus/ns)                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                Electric capacitance [Q^2][E^-1]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                blic static final double farad = coulomb/volt;	// farad = 6.24150e+24 * eplus/Megavolt                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                Magnetic Flux [T][E][Q^-1]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                blic static final double weber = volt*second;	// weber = 1000*megavolt*ns                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                Magnetic Field [T][E][Q^-1][L^-2]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                blic static final double tesla     = volt*second/meter2;	// tesla =0.001*megavolt*ns/mm2                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                Inductance [T^2][E][Q^-2]                
            </comment>
            <comment line="12">
                                
            </comment>
            <comment line="12">
                blic static final double henry = weber/ampere;	// henry = 1.60217e-7*MeV*(ns/eplus)**2                
            </comment>
            <comment line="12">
                                
            </comment>
